import java.util.ArrayList;

public class Solution {
	public static ArrayList<Integer> rotateArray(ArrayList<Integer> arr, int k) {
        // Write your code here.
        int n=arr.size();
        k=k%n;
        ArrayList<Integer> ans=new ArrayList<>();
        for(int i=k;i<n;i++){
            ans.add(arr.get(i));
        }
        for(int i=0;i<k;i++){
            ans.add(arr.get(i));
        }
        return ans;

    }
}

class Solution {
    void selectionSort(int[] arr) {
        // code here
        int min,temp,n=arr.length;
        for(int i=0;i<n-1;i++){
            min=i;
            for(int j=i;j<n;j++){
                if(arr[j]<arr[min])
                min=j;
            }
            temp=arr[min];
            arr[min]=arr[i];
            arr[i]=temp;
        }
    }
}
